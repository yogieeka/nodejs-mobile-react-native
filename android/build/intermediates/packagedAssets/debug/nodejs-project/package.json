{
  "name": "luna-middle-backend-ts",
  "version": "1.0.0",
  "description": "Luna middle backend from React to .NET sync",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "repository": "https://gitlab.com/luna/luna-middle-backend-ts",
  "license": "none",
  "private": true,
  "keywords": [],
  "scripts": {
    "describe": "npm-scripts-info",
    "build": "run-s clean build:*",
    "build:main": "tsc -p tsconfig.json && cp package.json build/package.json",
    "build:modules": "cd build && yarn --production --ignore-engines && cd ..",
    "fix": "run-s fix:*",
    "fix:tslint": "tslint --fix --project .",
    "test": "run-s build lint test:unit",
    "lint": "tslint --project .",
    "test:unit": "nyc --silent ava",
    "watch": "run-s clean build:main && run-p \"build:main -- -w\" \"test:unit -- --watch\"",
    "cov": "run-s build test:unit cov:html && opn coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:send": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "doc": "run-s doc:html && opn build/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "version": "standard-version",
    "reset": "git clean -dfx && git reset --hard && npm i",
    "clean": "trash build test",
    "all": "run-s reset test cov:check doc:html",
    "prepare-release": "run-s all version doc:publish",
    "start": "tslint --project . && ts-node src/index.ts",
    "dev": "ts-node-dev src/index.ts",
    "create-migration-db-setting": "node ./sequelize-auto-migration/makemigration --config-path=./sequelize-cli/db-setting.js",
    "create-migration-db-main": "node ./sequelize-auto-migration/makemigration --config-path=./sequelize-cli/db-main.js",
    "run-migration-db-setting": "sequelize db:migrate --options-path=./sequelize-cli/db-setting.js",
    "run-migration-db-main": "sequelize db:migrate --options-path=./sequelize-cli/db-main.js"
  },
  "scripts-info": {
    "info": "Display information about the package scripts",
    "build": "Clean and rebuild the project",
    "fix": "Try to automatically fix any linting problems",
    "test": "Lint and unit test the project",
    "watch": "Watch and rebuild the project on save, then rerun relevant tests",
    "cov": "Rebuild, run tests, then create and open the coverage report",
    "doc": "Generate HTML API documentation and open it in a browser",
    "doc:json": "Generate API documentation in typedoc JSON format",
    "version": "Bump package.json version, update CHANGELOG.md, tag release",
    "reset": "Delete all untracked files and reset the repo to the last commit",
    "prepare-release": "One-step: clean, build, test, publish docs, and prep a release",
    "start": "Start app directly by using ts-node"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "@nestjs/common": "^5.5.0",
    "@nestjs/core": "^5.5.0",
    "accounting": "^0.4.1",
    "axios": "^0.18.0",
    "axios-logger": "^1.0.0",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "continuation-local-storage": "^3.2.1",
    "core-js": "^2.6.1",
    "decimal.js": "^10.0.2",
    "dns": "^0.2.2",
    "express": "^4.16.4",
    "http-errors": "^1.7.1",
    "is-online": "8.0.0",
    "lodash": "^4.17.11",
    "mathjs": "^5.4.0",
    "moment": "^2.23.0",
    "money": "^0.2.0",
    "node-schedule": "^1.3.1",
    "node-thermal-printer": "^2.0.0",
    "path-to-regexp": "^2.4.0",
    "reflect-metadata": "^0.1.12",
    "regenerator-runtime": "^0.13.1",
    "request": "^2.88.0",
    "rxjs": "^6.3.3",
    "sequelize": "^4.42.0",
    "sequelize-typescript": "^0.6.6",
    "sha.js": "^2.4.11",
    "sha256": "^0.2.0",
    "sqlite3": "^4.0.4",
    "swagger-ui-express": "^4.0.2",
    "umzug": "^2.2.0",
    "uuid": "^3.3.2",
    "yargs": "^12.0.5",
    "yarn": "^1.13.0"
  },
  "devDependencies": {
    "@types/node": "^10.12.15",
    "@types/sequelize": "^4.27.33",
    "ava": "1.0.0-beta.7",
    "codecov": "^3.1.0",
    "command-line-args": "^5.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "deep-diff": "^1.0.2",
    "gh-pages": "^2.0.1",
    "glob": "^7.1.3",
    "js-beautify": "^1.8.9",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "object-hash": "^1.3.1",
    "opn-cli": "^4.0.0",
    "sequelize-cli": "^5.4.0",
    "standard-version": "^4.4.0",
    "sw2dts": "^2.6.1",
    "trash-cli": "^1.4.0",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-immutable": "^5.0.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6"
  },
  "ava": {
    "failFast": true,
    "files": [
      "build/**/*.spec.js"
    ],
    "require": [
      "./build/ava.js"
    ],
    "sources": [
      "build/**/*.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
